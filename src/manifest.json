{
  "name": "tabhistory",
  "version": "0.0",
  "manifest_version": 2,
  "description": "Visualize your tab history as a tree",
  "homepage_url": "http://www.elliotplant.com",
  "icons": {
  
  },
  "background": {
    "scripts": [
      "bg/HistreeStorage.js",
      "bg/tree_utils.js",
      "bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "tabhistory",
    "default_popup": "browser_action/browser_action.html"
  },
  "permissions": [
    "history",
    "tabs"
  ]
}
