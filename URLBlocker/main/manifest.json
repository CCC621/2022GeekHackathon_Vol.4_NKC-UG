{
  "name": "URLBlocker",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Exsample Chrome Extension",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./js/script.js"
      ]
    }
  ],
  "permissions": [
  	"tabs"
  ],
  "action": {
      "default_icon": "icon.png",
      "default_title": "MyExtension",
      "default_popup": "BlockPage.html"
  }
}
